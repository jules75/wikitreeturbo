{
  "manifest_version": 2,
  "name": "WikiTree FaceID",
  "description": "Match faces in WikiTree photos to their profiles",
  "version": "0.1",
  "author": "julianlaffey@gmail.com",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "faceid.css"
      ],
      "js": [
        "lib/lodash.js",
        "lib/jquery-3.2.1.min.js",
        "faceid.js"
      ],
      "matches": [
        "https://www.wikitree.com/photo/*"
      ]
    }
  ]
}