{
  "manifest_version": 2,
  "name": "WikiTree Turbo",
  "description": "Additional features for WikiTree genealogy site.",
  "version": "0.1",
  "author": "julianlaffey@gmail.com",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "features/faceid/faceid.css"
      ],
      "js": [
        "lib/lodash.js",
        "lib/jquery-3.2.1.min.js",
        "features/faceid/faceid.js"
      ],
      "matches": [
        "https://www.wikitree.com/photo/*"
      ]
    },
    {
      "css": [
        "features/collapse/collapse.css"
      ],
      "js": [
        "lib/lodash.js",
        "lib/jquery-3.2.1.min.js",
        "features/collapse/collapse.js"
      ],
      "matches": [
        "https://www.wikitree.com/*"
      ]
    }
  ]
}